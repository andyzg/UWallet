package com.enghack.watcard;

import java.util.ArrayList;

public class WatcardInfo {

	ArrayList<Transaction> transList;
	
	int studentID;
	int PIN;
	
	double flexDollar;
	double mealDollar;
	
	public WatcardInfo(ArrayList<Transaction> list, int studentID, int PIN)
	{
		this.transList = list;
		this.studentID = studentID;
		this.PIN = PIN;
	}
}
